    "def my_Upper_lower(U):\n",
    "    w=\"\"\n",
    "    for u in U:\n",
    "        asc=ord(u)\n",
    "        if 97<=ord(u)<=122:\n",
    "            U=u.upper()\n",
    "        if 65<=ord(u)<=90:\n",
    "            U=u.lower()\n",
    "        w=w+U\n",
    "    return(w)\n",
    "        \n",
    "A=input(\"請輸入英文單字:\")\n",
    "my_Upper_lower(A)\n",
    "print(my_Upper_lower(A))"
